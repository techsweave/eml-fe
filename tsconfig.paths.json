{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@models/*": [
                "src/models/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@libs/*": [
                "src/libs/*"
            ],
            "@pages/*": [
                "src/pages/*"
            ],
            "@public/*": [
                "public/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@api/*": [
                "src/pages/api/*"
            ],
        }
    }
}